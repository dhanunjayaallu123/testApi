pipeline {
agent any
tools{
maven "Maven"
}
stages{
stage('initialize mvn versionn'){
steps{
bat 'mvn --version'
}
}
stage( 'git cloning'){
steps{
git branch: 'main', url: 'https://github.com/dhanunjayaallu123/testApi.git'
}
}
stage('mvn build'){
steps{
bat 'mvn compile'
}
}
stage('jenerate jar file'){
steps{
bat 'mvn package'
}
}
}
}
